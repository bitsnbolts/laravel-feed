{
  "name": "bitsnbolts/laravel-feed",
  "description": "Bitsnbolts Feed package for Laravel.",
  "homepage": "https://bitsnbolts.com",
  "keywords": [
    "bitsnbolts",
    "laravel",
    "feed",
    "generator",
    "rss",
    "atom"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Thijs van den Anker",
      "email": "thijs@bitsnbolts.nl",
      "role": "Developer",
      "homepage": "https://bitnbolts.nl"
    }
  ],
  "support": {
    "issues": "https://github.com/bitsnbolts/laravel-feed/issues",
    "source": "https://github.com/bitsnbolts/laravel-feed",
    "wiki": "https://github.com/bitsnbolts/laravel-feed/wiki"
  },
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0",
    "illuminate/filesystem": "^11.0"
  },
  "require-dev": {
    "laravel/framework": "^11.0",
    "phpunit/phpunit": "^11.0",
    "orchestra/testbench-core": "^9.0"
  },
  "autoload": {
    "psr-0": {
      "Bitsnbolts\\Feed": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bitsnbolts\\Feed\\FeedServiceProvider"
      ],
      "aliases": {
        "Feed": "Bitsnbolts\\Feed\\Feed"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
